! Create the window with the specified X window geometry [default 80x24]; option -geometry.
URxvt*geometry: 170x55

! Use the specified colour to display underlined characters when the foreground colour is the
URxvt*colorUL: #77bee0

! Select the fonts to be used. This is a comma separated list of font names that are checked in order
URxvt*font: xft:FantasqueSansMono:size=10

! Set window title string, the default title is the command-line specified after the -e option, if
URxvt*title: urxvt

! True: enable the scrollbar [default]; option -sb. False: disable the scrollbar; option +sb.
URxvt*scrollBar: false

URxvt*perl-ext-common: default,matcher,fullscreen
URxvt.matcher.button: 1
URxvt.keysym.F11: perl:fullscreen:switch

! Specifies the program to be started with a URL argument. Used by the "selection-popup" and
URxvt*url-launcher: opera

